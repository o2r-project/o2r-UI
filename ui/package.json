    "archiver": "^5.3.0",
    "core-js": "^3.8.1",
    "csv-string": "^4.0.1",
    "formik": "^1.5.8",
    "formik-persist": "^1.1.0",
    "jquery": "^3.5.1",
    "jquery-highlight": "^3.5.0",
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4",
    "material-ui-chip-input": "^2.0.0-beta.2",
    "material-ui-dropzone": "^3.5.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-compare-image": "^3.0.2",
    "react-dom": "^17.0.1",
    "react-highlight.js": "^1.0.7",
    "react-iframe": "^1.8.0",
    "react-json-tree": "^0.13.0",
    "react-leaflet": "^2.7.0",
    "react-leaflet-draw": "0.19.0",
    "react-modal-video": "^1.2.3",
    "react-reflex": "^3.0.16",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^4.0.1",
